<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    
    <!-- Fondo principal -->
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#010B19" />
        </shape>
    </item>
    
    <!-- Rectángulo exterior -->
    <item android:left="28dp" android:top="14dp">
        <shape android:shape="rectangle">
            <solid android:color="#010B19" />
            <stroke android:width="2dp" android:color="#B4B1B8" />
            <corners android:topRightRadius="8dp" android:bottomLeftRadius="8dp" />
        </shape>
    </item>
    
    <!-- Rectángulo central -->
    <item android:left="21dp" android:top="36dp">
        <shape android:shape="rectangle">
            <solid android:color="#010B19" />
            <stroke android:width="2dp" android:color="#B4B1B8" />
            <corners android:bottomLeftRadius="5dp" android:bottomRightRadius="5dp" android:topRightRadius="5dp" />
        </shape>
    </item>
    
    <!-- Rectángulo superior -->
    <item android:left="21dp" android:top="1dp">
        <shape android:shape="rectangle">
            <solid android:color="#010B19" />
            <stroke android:width="2dp" android:color="#B4B1B8" />
            <corners android:topLeftRadius="8dp" android:topRightRadius="8dp" />
        </shape>
    </item>
    
    <!-- Botón Home -->
    <item android:left="1dp" android:top="42dp">
        <shape android:shape="rectangle">
            <solid android:color="#010B19" />
            <stroke android:width="2dp" android:color="#B4B1B8" />
            <corners android:radius="8dp" />
        </shape>
    </item>
    
    <!-- Botón Notificaciones -->
    <item android:left="252dp" android:top="42dp">
        <shape android:shape="rectangle">
            <solid android:color="#010B19" />
            <stroke android:width="2dp" android:color="#B4B1B8" />
            <corners android:radius="8dp" />
        </shape>
    </item>
    
    <!-- Imagen de perfil -->
    <item android:left="300dp" android:top="42dp">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
            <stroke android:width="2dp" android:color="#B4B1B8" />
            <corners android:radius="8dp" />
        </shape>
    </item>
    
    <!-- Iconos decorativos -->
    <item android:left="167dp" android:top="4.5dp">
        <shape android:shape="oval">
            <solid android:color="@android:color/transparent" />
            <stroke android:width="2dp" android:color="#B4B1B8" />
        </shape>
    </item>
    
    <item android:left="192dp" android:top="4.5dp">
        <shape android:shape="oval">
            <solid android:color="@android:color/transparent" />
            <stroke android:width="2dp" android:color="#B4B1B8" />
        </shape>
    </item>
    
    <!-- Círculo decorativo -->
    <item android:left="245dp" android:top="7dp">
        <shape android:shape="oval">
            <solid android:color="@android:color/transparent" />
            <stroke android:width="2dp" android:color="#B4B1B8" />
        </shape>
    </item>
    
    <!-- Línea horizontal -->
    <item android:left="265dp" android:top="18dp">
        <shape android:shape="rectangle">
            <solid android:color="#B4B1B8" />
        </shape>
    </item>

</layer-list>
